<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/vector1"
    >

    <ImageView
        android:id="@+id/im"
        android:layout_width="410dp"
        android:layout_height="230dp"
        android:layout_marginRight="500dp"
        android:src="@drawable/thank" />



    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="230dp"
        android:background="#00FFC4"
        android:padding="6dp"
        android:text="We would like to thank the following websites for extending their support to us"
        android:textColor="#000000"
        android:textSize="20dp"
        app:fontFamily="@font/alike_angular" />



    <ImageView
        android:id="@+id/imtt"
        android:layout_width="170dp"
        android:layout_height="170dp"
        android:layout_marginTop="500dp"
        android:layout_marginLeft="210dp"
        android:background="#FFFFFF"
        android:layout_marginBottom="10dp"
        android:paddingBottom="5dp"
        android:paddingTop="5dp"
        android:paddingLeft="3dp"
        android:src="@drawable/s" />

    <ImageView
        android:id="@+id/imyt"
        android:layout_width="170dp"
        android:layout_height="160dp"
        android:layout_marginTop="310dp"
        android:layout_marginLeft="10dp"
        android:background="#FFFFFF"
        android:paddingBottom="5dp"
        android:paddingTop="5dp"
        android:paddingLeft="3dp"
        android:src="@drawable/cc" />

    <ImageView
        android:id="@+id/it"
        android:layout_width="170dp"
        android:layout_height="160dp"
        android:layout_marginTop="310dp"
        android:layout_marginLeft="210dp"
        android:background="#FFFFFF"
        android:paddingBottom="10dp"
        android:paddingTop="5dp"
        android:paddingLeft="3dp"
        android:src="@drawable/cb" />

    <ImageView
        android:id="@+id/t"
        android:layout_width="170dp"
        android:layout_height="170dp"
        android:layout_marginTop="500dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="5dp"
        android:background="#FFFFFF"
        android:paddingBottom="5dp"
        android:paddingTop="5dp"
        android:paddingLeft="3dp"
        android:src="@drawable/cf" />

</RelativeLayout>